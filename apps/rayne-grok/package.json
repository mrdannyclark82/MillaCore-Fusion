    "dev": "NODE_ENV=development npx tsx server/index.ts",
    "dev:full": "NODE_ENV=development npx tsx server/index.ts",
    "cli": "npx tsx cli/milla-cli.ts",
    "build": "npx vite build && npx esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "npx tsc",
    "db:push": "npx drizzle-kit push",
    "migrate:memory": "npx tsx server/migrateToSqlite.ts",
    "migrate:encrypt": "npx tsx server/encryptMigration.ts",
    "postinstall": "npm rebuild better-sqlite3",
    "test": "vitest run --config ./vitest.config.server.ts",
    "test:watch": "vitest watch --config ./vitest.config.server.ts",
    "test:coverage": "vitest run --coverage --config ./vitest.config.server.ts",
    "test:ui": "vitest --ui --config ./vitest.config.server.ts",
    "lint": "npx eslint . --ext .ts,.tsx",
    "format": "npx prettier --write .",
    "docs:generate": "npx typedoc",
    "docs:watch": "npx typedoc --watch"
